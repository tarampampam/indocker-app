{
  "name": "monitor-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "node --no-deprecation ./node_modules/.bin/openapi-typescript ./../api/openapi.yml --immutable --enum --array-length --additional-properties=false --output ./src/api/schema.gen.ts",
    "fmt": "prettier --write ./*.{js,ts} ./src && npm run lint:es -- --fix",
    "lint": "npm run lint:ts && npm run lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint ./src/**/*.{ts,tsx}",
    "test": "vitest --run",
    "serve": "vite --strictPort",
    "watch": "vite build --watch",
    "build": "tsc --noEmit && vite build"
  },
  "dependencies": {
    "framer-motion": "^12.4.7",
    "openapi-fetch": "^0.13.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@fetch-mock/vitest": "^0.2.7",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "openapi-typescript": "^7.6.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.5"
  },
  "volta": {
    "node": "22"
  },
  "engines": {
    "node": ">=22"
  }
}
