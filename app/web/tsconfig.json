{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "src/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "service-worker.ts"
  ],
  "compilerOptions": {
    "strict": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "lib": [
      "esnext",
      "webworker"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
